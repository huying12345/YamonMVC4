@section styles{
    <link href="/Resources/JsLib/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <style>
        .panel-body {
            padding: 0px !important;
        }
    </style>

}
@section scripts{
    <script src="/Resources/JsLib/bootstrap/js/bootstrap.js"></script>
}
<div region="center" border="true" style="padding: 10px; background: #F6F6F6; border: 1px solid #ccc;">

    <div class="panel panel-primary">
        <!-- Default panel contents -->
        <div class="panel-heading">预约订单信息</div>
        <!-- Table -->
        <table class="table">
            <thead>
                <tr>
                    <th>
                        预约订单编号
                    </th>

                        <th>
                            会员编号
                        </th>
    
                    <th>
                        总金额
                    </th>

                    <th>
                        创建时间
                    </th>
                    <th>
                        备注
                    </th>

                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>@(Html.Raw(Model.ProductBookID))</td>

                        <td>@(Html.Raw(Model.MemberID))</td>

                    <td>@(Html.Raw(Model.TotalMoney))</td>
                    <td>@(Html.Raw(Model.CreateTime))</td>
                    <td>@(Html.Raw(Model.Comment))</td>
                </tr>
            </tbody>
        </table>
    </div>

    <div class="panel panel-primary">
        <!-- Default panel contents -->
        <div class="panel-heading">订单明细</div>
        <!-- Table -->
        <table class="table">
            <thead>
                <tr>
                    <th>
                 预约编号
                    </th>
                    <th>
                        商品名称
                    </th>

                    <th>
                        原价
                    </th>

                    <th>
                        数量
                    </th>
                    <th>
                        预约时间
                    </th>
                </tr>
            </thead>
            <tbody>

                @foreach (var item in ViewBag.List)
                {
                    <tr>
                        <td>@(item.ProductBookDetailID)</td>
                        <td>@(item.ProductName)</td>
                        <td>@(item.Price)</td>
                        <td>@(item.Num)</td>
                        <td>@(item.MakeTime)</td>

                    </tr>
                }

            </tbody>

        </table>
    </div>



    @RenderPage("~/Areas/Product/Views/OrderDetail/Extend/Model_ViewHtml.cshtml")

</div>
<div region="south" border="false" style="text-align: right; background: #F6F6F6;height: 30px; line-height: 30px;">
    <a id="btnCancel" class="easyui-linkbutton" icon="icon-cancel" href="javascript:CloseDialog();">
        关闭
    </a>
</div>

